<!DOCTYPE html>
<html>
<head>
    <title>TickTech</title>
    <link rel="stylesheet" type="text/css" href="/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="/css/login.css">
</head>
<body>
<div class="container">
    <div class="row"></div>
    <div class="row">
        <div class="col-lg-4">
        </div>
        <div class="col-lg-4">
            {{{body}}}
        </div>
        <div class="col-lg-4">
        </div>
    </div>
    <div class="row">
        <div class="col-lg-4"></div>
        <div class="col-lg-4">
            {{#if success_msg}}
                <div class="msg msg-success">{{success_msg}}</div>
            {{/if}}

            {{#if error_msg}}
                <div class="msg msg-danger">{{error_msg}}</div>
            {{/if}}

            {{#if error}}
                <div class="msg msg-danger-text">{{error}}</div>
            {{/if}}
        </div>
    </div>
</div>
</body>
</html>